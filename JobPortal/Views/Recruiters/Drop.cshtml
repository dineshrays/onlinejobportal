@model JobPortal.ViewModel.CountryViewModel
@{
    ViewBag.Title = "Drop";
}

<h2>Drop</h2> 

<div class="form-horizontal">
    <div class="row form-group">
        

        <label class="control-label col-md-2">Country </label>

        <div class="col-12 col-md-9">
            @Html.DropDownListFor(model => model.country.Id, ViewData["cuntrlist"] as SelectList, "Select Country", new { @class = "form-control", @id = "scrnId" })

        </div>
    </div>
</div>